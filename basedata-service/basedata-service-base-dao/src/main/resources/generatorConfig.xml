<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="saas" targetRuntime="MyBatis3DynamicSql">
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
<!--        <plugin type="com.bosssoft.nontax3.saas.compoent.mybatis.generator.plugin.JSR303Plugin" />-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://114.55.137.13:9000/nontax"
                        userId="root"
                        password="123456"/>
        <javaTypeResolver>
            <property name="useJSR310Types" value="true" />
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.hyd.basedata.entity" targetProject="src/main/java"/>
        <javaClientGenerator targetPackage="com.hyd.basedata.dao" targetProject="src/main/java"/>
        <table tableName="basic_unit" mapperName="UnitBaseMapper">
            <domainObjectRenamingRule searchString="^Basic" replaceString=""/>
        </table>

    </context>
</generatorConfiguration>
